  <div class="px-3 py-1 text-white" style="background: rgb(49, 151, 149)">
    <div class="container">
      <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
        <a to="/" class="d-flex align-items-center my-1 my-lg-0 me-lg-auto text-white text-decoration-none">
          <img src="assets/logo.png" height="60" width="65" />
        </a>
        <ul class="nav col-12 col-lg-auto my-1 justify-content-center my-md-0 text-small">
          <li>
            <a href="/" class="nav-link text-white">
              <i class="fas fa-house-user bi d-block mx-2 mb-1"></i>
              Home
            </a>
          </li>
          <% if (user){ %>
            <li>

              <a href="/profile/leaderboard" class="nav-link text-white">
                <i class="fas fa-trophy bi d-block mx-4 mb-1"></i>
                Leaderboard
              </a>

            </li>
            <li>

              <a href="/profile/tasks" class="nav-link text-white">
                <i class="fas fa-tasks bi d-block mx-2 mb-1"></i>
                Tasks
              </a>
            </li>
            <li>

              <a href="/profile/user" class="nav-link text-white">
                <i class="fas fa-address-card bi d-block mx-2 mb-1"></i>
                Profile
              </a>

            </li>
            <li>

              <a class="nav-link text-white">
                <span class="changedesc my-auto">Hello <%= user.yourname%></span>
              </a>

            </li>
            <li>
              <a class="nav-link text-white" href="/logout"
                style="text-decoration: none;color: white;font-family: 'Roboto';font-weight: 900;">

                <button class="btn btn-warning text-white me-2"><i class="fas fa-sign-out-alt me-2 g-icon-x"></i><span
                    style="text-decoration: none;color: white;font-family: 'Roboto';font-weight: 900;">Logout</span></button>
              </a>

            </li>

            <%}%>
        </ul>
        <% if (!user){ %>
          <span class="changedesc">Hello Guest</span>
          <span class="me-4"></span>
          <button class="btn btn-warning text-white me-2"><a href="/google"
              style="text-decoration: none;color: white;font-family: 'Roboto';"><i
                class="fab fa-google me-2 g-icon"></i>Login with Google</a></button>
          <%}%>
      </div>
    </div>
